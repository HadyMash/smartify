    "generate-jwe-key": "ts-node src/generate-jwe-encryption-key.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier . --write --ignore-path .gitignore --log-level warn --ignore-unknown",
    "format:check": "prettier . --check --ignore-path .gitignore --log-level warn --ignore-unknown",
    "lint": "eslint ./src --exit-on-fatal-error",
    "lint:fix": "eslint ./src --fix --exit-on-fatal-error"
  },
  "keywords": [],
